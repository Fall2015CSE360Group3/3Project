<h1>Doctors#edit</h1>
<p>Find me in app/views/doctors/edit.html.erb</p>
<%= form_tag(:action => 'edit_submit', :id => @editDoctor[:id]) do %>
<p>First Name:</br><%= text_field_tag :first_name, @editDoctor[:name_first] %></p>
<p>Last Name:</br><%= text_field_tag :last_name, @editDoctor[:name_last] %></p>
<p>Specialty:</br><%= text_field_tag :specialty, @editDoctor[:specialty] %></p>
<%= submit_tag("Save Changes")%>
<% end %>