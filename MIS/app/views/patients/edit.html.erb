<h1>Patients#edit</h1>
<p>Find me in app/views/patients/edit.html.erb</p>
<%= form_tag(:action => 'edit_submit', :id => @editPatient[:id]) do %>
<p>First Name:</br><%= text_field_tag :first_name, @editPatient[:name_first] %></p>
<p>Last Name:</br><%= text_field_tag :last_name, @editPatient[:name_last] %></p>
<p>Gender:</br><%= text_field_tag :gender, @editPatient[:gender] %></p>
<p>Social Security Number:</br><%= text_field_tag :ssn, @editPatient[:ssn] %></p>
<p>Address:</br><%= text_field_tag :address, @editPatient[:address] %></p>
<p>Phone:</br><%= telephone_field "", :phone, :value => @editPatient[:phone] %></p>
<p>Date of Birth:</br><%= date_field "", :dob, :value => @editPatient[:dob] %></p>
<p>Weight:</br><%= number_field "",:weight, :value => @editPatient[:weight], step: 0.01 %></p>
<p>Height:</br><%= number_field "",:height, :value => @editPatient[:height], step: 0.01 %></p>
<%= submit_tag("Save Changes")%>
<% end %>