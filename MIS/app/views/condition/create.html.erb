<h1>Report Condition</h1>
<p></p>
<%= form_tag(:action => 'create_submit', :patient_id => params[:patient_id]) do %>
<p>Condition:<%= collection_select("", :condition, @symptoms, :to_s, :to_s) %></p>
<% if @show_doctors_notes == true %>
<p>Notes: </br><%= text_area("", :notes, cols: 60, rows: 40) %><p>
<% end %>
<%= submit_tag("Create")%>
<% end %>
